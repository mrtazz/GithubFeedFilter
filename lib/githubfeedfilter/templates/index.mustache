<div id="feeditems">
<ul>
{{#items}}

<li>
  <span class="time">{{time}} {{timeunit}} ago: </span>
  <a href="https://github.com/{{actor}}">{{actor}}</a>

{{#PullRequestEvent}}
  <a href="{{url}}">{{action}}</a> Pull Request {{number}} at
{{/PullRequestEvent}}
{{#IssuesEvent}}
  {{action}} <a href="{{url}}"> issue {{number}}</a> at
{{/IssuesEvent}}
{{#IssueCommentEvent}} commented on an issue at {{/IssueCommentEvent}}
{{#CommitCommentEvent}} commented on commit {{number}} at {{/CommitCommentEvent}}
{{#BranchEvent}}
  {{#CreateEvent}}
    created branch <a href="{{url}}">{{object}}</a> at
{{/CreateEvent}}
  {{#DeleteEvent}}
    deleted branch <a href="{{url}}">{{object}}</a> at
  {{/DeleteEvent}}
{{/BranchEvent}}
{{#TagEvent}}
  {{#CreateEvent}}
    created tag <a href="{{url}}">{{object}}</a> at
  {{/CreateEvent}}
  {{#DeleteEvent}}
    deleted tag <a href="{{url}}">{{object}}</a> at
  {{/DeleteEvent}}
{{/TagEvent}}
{{#FollowEvent}} started following {{followee}} {{/FollowEvent}}
{{#ForkApplyEvent}} applied fork commits at {{/ForkApplyEvent}}
{{#PushEvent}} <a href="{{url}}">pushed</a> to {{/PushEvent}}
{{#ForkEvent}} forked {{/ForkEvent}}
{{#WatchEvent}} started watching {{/WatchEvent}}

{{^FollowEvent}}
  <a href="{{repourl}}"> {{repo}}</a>
{{/FollowEvent}}
</li>
{{/items}}
</ul>
</div>
